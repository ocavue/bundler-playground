    "esbuild": "^0.27.3",
    "rolldown": "^1.0.0-rc.5",
    "rolldown-plugin-dts": "^0.22.1",
    "rollup": "^4.57.1",
    "tsdown": "^0.20.3",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  }
